        <StackPanel Orientation="Horizontal" Grid.Row="1" Margin="10,705,918,14" Grid.ColumnSpan="2" Grid.RowSpan="2"
                    Visibility="Hidden">
            <Label Content="URL to get elements:" Margin="0,3,5,4" VerticalAlignment="Center"
                   VerticalContentAlignment="Center"
                   HorizontalContentAlignment="Right" HorizontalAlignment="Right" Height="26" />
            <TextBox Name="TextBoxURL1" Margin="5,18" Height="24" Width="400" Text="" VerticalAlignment="Center"
                     VerticalContentAlignment="Center" TextWrapping="Wrap" />
        </StackPanel>


        <StackPanel Orientation="Horizontal" Grid.Row="3">
            <Label Name="LabelFoundElements" Content="Found Elements:" VerticalContentAlignment="Center"
                   FontWeight="Bold" />
        </StackPanel>
        <Button Click="ButtonGetElements_Click" Name="ButtonGetElements" Style="{DynamicResource MyButtonStyle}"
                Grid.Row="3" Width="200" IsEnabled="false" VerticalAlignment="Bottom" HorizontalAlignment="Left"
                HorizontalContentAlignment="Center" Content="Get Elements" TabIndex="7" Grid.Column="1"
                Margin="10,0,0,26" />


        <StackPanel Orientation="Horizontal" Margin="10,0,0,0" Grid.Column="1" Grid.Row="3">
            <Label Name="LabelTestCase" Content="Test Case: " Margin="5,0,0,0" Height="28" Visibility="Visible"
                   HorizontalContentAlignment="Right" VerticalAlignment="Center" FontWeight="Bold" />
            <TextBox Name="TextBoxTestCase" Visibility="Visible" Width="100" Height="24" VerticalAlignment="Center"
                     VerticalContentAlignment="Center" HorizontalAlignment="Left" HorizontalContentAlignment="Left"
                     TabIndex="88" />
            <Label Name="LabelTestPage" Content="Test Page:" Height="28" FontWeight="Bold" Visibility="Visible"
                   HorizontalContentAlignment="Right" VerticalAlignment="Center" />
            <TextBox Name="TextBoxTestCasePage" Visibility="Visible" Width="50" Height="24" VerticalAlignment="Center"
                     VerticalContentAlignment="Center" HorizontalAlignment="Left" HorizontalContentAlignment="Left"
                     TabIndex="89" />
        </StackPanel>

        <StackPanel Orientation="Horizontal" Margin="215,0,0,0" Grid.Row="3" Grid.Column="1" HorizontalAlignment="Left"
                    Width="1387">
            <CheckBox Name="CheckBoxObjectVisibility" Content="Object Visibility" VerticalAlignment="Center"
                      Visibility="Hidden" />

            <Label Name="LabelTestSelectFrom" Content="Select ID from: " Margin="5,0,0,0" Height="28"
                   Visibility="Visible"
                   HorizontalContentAlignment="Right" VerticalAlignment="Center" FontWeight="Bold" />
            <TextBox Name="TextBoxSelectFrom" Visibility="Visible" Width="75" Height="24" VerticalAlignment="Center"
                     VerticalContentAlignment="Center" HorizontalAlignment="Left" HorizontalContentAlignment="Left"
                     TabIndex="8" TextChanged="TextBoxSelectFrom_TextChanged" LostFocus="TextBoxSelectFrom_LostFocus" />
            <Label Name="LabelTestSelectTo" Content="Select ID till:" Height="28" FontWeight="Bold"
                   Visibility="Visible"
                   HorizontalContentAlignment="Right" VerticalAlignment="Center" />
            <TextBox Name="TextBoxTestSelectTo" Visibility="Visible" Width="75" Height="24" VerticalAlignment="Center"
                     VerticalContentAlignment="Center" HorizontalAlignment="Left" HorizontalContentAlignment="Left"
                     TabIndex="9" TextChanged="TextBoxTestSelectTo_TextChanged"
                     LostFocus="TextBoxTestSelectTo_LostFocus" />

            <Button Click="ButtonSelectAllClick" x:Name="ButtonSelectAll" Style="{DynamicResource MyButtonStyle}"
                    Width="105" Content="Select All" IsEnabled="false" VerticalAlignment="Center"
                    HorizontalAlignment="Center" HorizontalContentAlignment="Center" Margin="15,0,25,0" />
            <Button Click="ButtonSetToTest_Click" IsEnabledChanged="ButtonSetToTest_IsEnabledChanged"
                    Name="ButtonSetToTest" Style="{DynamicResource MyButtonStyle}"
                    Width="175" IsEnabled="true" VerticalAlignment="Center" HorizontalAlignment="Right"
                    HorizontalContentAlignment="Center" Content="Add to test steps" TabIndex="9" />
            <CheckBox Name="CheckBoxInlogIncluded" Content="Login included" VerticalContentAlignment="Center"
                      Click="CheckBoxInlogIncluded_Click" Width="99" />
            <Button Name="CheckTextAll"
                    Style="{DynamicResource MyButtonStyle}"
                    VerticalAlignment="Center"
                    HorizontalAlignment="Right"
                    HorizontalContentAlignment="Center"
                    Content="Select All Text"
                    Margin="5,24,5,25"
                    Width="110"
                    IsEnabled="False"
                    Click="CheckTextAll_Click" />
            <CheckBox Margin="0,0,0,0" Name="CheckBoxSwitchTo" Visibility="Hidden" Content="Switch to URL"
                      VerticalContentAlignment="Center" Width="100" />
            <CheckBox Name="ButtonCSV"
                      Content="Save as CSV"
                      VerticalAlignment="Center"
                      HorizontalAlignment="Right"
                      Margin="50,0,0,0" />

            <Button Name="ButtonTotCSV" IsEnabled="False" Style="{DynamicResource MyButtonStyle}" Content="Save as CSV" Click="ButtonTotCSV_Click" />

        </StackPanel>

        <TextBox Name="TextBoxElementen" Visibility="Hidden" Width="900" Height="600" Grid.RowSpan="5"
                 Grid.ColumnSpan="5" />
        <DataGrid LostFocus="DataGridElementsLostFocus"
                  Width="1700"
                  Name="DataGridElements"
                  Grid.Row="2"
                  Grid.ColumnSpan="2"
                  AlternatingRowBackground="{DynamicResource DataGridAchtergrondAlternatief}"
                  Background="{DynamicResource DataGridAchtergrondAlternatief}"
                  Visibility="Hidden"
                  CanUserResizeColumns="true"
                  CanUserResizeRows="true"
                  AutoGenerateColumns="false"
                  CanUserSortColumns="true"
                  RowHeaderWidth="0"
                  HorizontalScrollBarVisibility="Visible"
                  IsSynchronizedWithCurrentItem="False"
                  VerticalScrollBarVisibility="Visible"
                  HorizontalAlignment="Center"
                  CanUserAddRows="False"
                  CanUserDeleteRows="False"
                  CanUserReorderColumns="False"
                  Height="700"
                  CurrentCellChanged="DataGridElements_CurrentCellChanged" RowHeight="24">
            <DataGrid.Columns>
                <DataGridTextColumn Header="ID" IsReadOnly="true" Binding="{Binding idSelenium_element}" />
                <DataGridCheckBoxColumn Header="Select" IsReadOnly="false"
                                        Binding="{Binding elementCheck, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                <DataGridTextColumn Header="url" Visibility="Hidden" IsReadOnly="true" Binding="{Binding elementLink}" />
                <DataGridTextColumn Header="Id" IsReadOnly="true" Binding="{Binding elementId}" />
                <DataGridTextColumn Header="Name" IsReadOnly="true" Binding="{Binding elementName}" />
                <DataGridTextColumn Header="XPath" IsReadOnly="true" Binding="{Binding elementXpath}" MaxWidth="500" />
                <DataGridTextColumn Header="Tagname" IsReadOnly="true" Binding="{Binding elementTagname}" />
                <DataGridTextColumn Header="Text" IsReadOnly="true" Binding="{Binding elementText}"  MaxWidth="500"/>
                <DataGridCheckBoxColumn Header="Check Text" IsReadOnly="false"
                                        Binding="{Binding elementCheckText, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                <DataGridTextColumn Header="Class" IsReadOnly="true" Binding="{Binding elementClass}" />
                <DataGridTextColumn Header="Href" IsReadOnly="true" Binding="{Binding elementHref}" />
            </DataGrid.Columns>
        </DataGrid>
        <Button Name="ButtonJSON"
                Content="Create JSON-datafile"
                Grid.Row="5"
                Grid.Column="2"
                Margin="0,0,75,0"
                Style="{DynamicResource MyButtonStyle}"
                Width="175"
                IsEnabled="False"
                VerticalAlignment="Center"
                HorizontalAlignment="Right"
                HorizontalContentAlignment="Center" Click="ButtonJSON_Click"
                Visibility="Hidden" />
    </Grid>
</Window>