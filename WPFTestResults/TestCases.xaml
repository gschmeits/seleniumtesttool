                    VerticalContentAlignment="Center" Click="SearchButton_Click" />
                <Button x:Name="ClearSearchButton" Style="{DynamicResource MyButtonStyle}" 
                    Height="24" 
                    Content="Clear" 
                    VerticalContentAlignment="Center" Click="ClearSearchButton_Click"  />
            </StackPanel>

            <StackPanel Margin="15,0,0,0" 
