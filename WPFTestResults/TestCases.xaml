                <Label Content="TestNr.:" VerticalContentAlignment="Center" />
                <ComboBox Name="ComboboxSelectNr" Width="73" Height="28" VerticalContentAlignment="Center"
                          DropDownClosed="ComboboxSelectNr_DropDownClosed" Margin="0,10" />
                <Button Name="ButtonXpath" Content="Use XPath" Margin="15,0" Width="100" Style="{DynamicResource MyButtonStyle}" Click="ButtonXpath_Click"></Button>
            </StackPanel>

            <StackPanel Orientation="Horizontal"
