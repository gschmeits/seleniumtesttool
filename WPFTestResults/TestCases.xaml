                            <Setter Property="TextBlock.VerticalAlignment"
                                    Value="Center" />
                            <Setter Property="TextBlock.Margin"
                                    Value="5,0" />
                        </Style>
                    </DataGridTextColumn.ElementStyle>
                </DataGridTextColumn>

                <DataGridTextColumn Header="Href"
                                    Binding="{Binding href}"
                                    Width="auto"
                                    Visibility="Visible" />
                <DataGridTextColumn Header="src"
