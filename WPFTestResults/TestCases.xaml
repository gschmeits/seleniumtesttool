                    <DataGridTextColumn.ElementStyle>
                        <Style>
                            <Setter Property="TextBlock.TextWrapping"
                                    Value="Wrap" />
                            <Setter Property="TextBlock.TextAlignment"
                                    Value="Left" />
                            <Setter Property="TextBlock.VerticalAlignment"
                                    Value="Center" />
                            <Setter Property="TextBlock.Margin"
